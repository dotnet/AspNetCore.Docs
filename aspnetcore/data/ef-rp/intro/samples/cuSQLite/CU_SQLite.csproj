  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>$(DefineConstants)TRACE;SQLiteVersion</DefineConstants>
  </PropertyGroup>

  <ItemGroup>
