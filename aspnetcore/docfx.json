        "src": "../_razor/docs",
        "dest": "_razor/docs"
      }
    ],
    "resource": [
      {
        "files": [ "**/*.png", "**/*.jpg", "**/*.gif", "**/*.svg", "**/*.pdf", "**/zone-pivot-groups.json" ],
        "exclude": [ "**/includes/**", "**/obj/**", "_site/**" ],
        "group": "group1",
        "src": "."
      }
    ],
    "overwrite": [],
    "externalReference": [],
