CREATE TABLE [dbo].[Movie] (
    [Id]          INT             NOT NULL IDENTITY,
    [Title]       TEXT            NULL,
    [ReleaseDate] DATETIME2            NOT NULL,
    [Genre]       VARCHAR(30)            NULL,
    [Price]       DECIMAL (18, 2) NOT NULL,
    [Rating]      TEXT            NULL,
    [Director] VARCHAR(MAX) NULL, 
    CONSTRAINT [PK_Movie] PRIMARY KEY CLUSTERED ([Id] ASC)
);

